                        android:textAlignment="center"
                        android:textColor="@color/colorTextDark"
                        android:textSize="10dp"
                        android:textStyle="normal" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/loginpart6"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_below="@id/loginpart5"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:background="@null"
                android:gravity="center_horizontal"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/line31"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="0dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btnrefundterms"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_centerHorizontal="true"
                        android:background="@layout/round_corner_image_button_main"
                        android:backgroundTint="#ffc107"
                        android:gravity="center"
                        android:padding="17dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/ico_refund"
                        android:tint="@color/white" />

                    <TextView
                        android:id="@+id/lblrefund"
                        android:layout_width="75dp"
                        android:layout_height="25dp"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:text="@string/refund_policy"
                        android:textAlignment="center"
                        android:textColor="@color/colorTextDark"
                        android:textSize="10dp"
                        android:textStyle="normal" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/line32"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="0dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btndataprivacy"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_centerHorizontal="true"
                        android:background="@layout/round_corner_image_button_main"
                        android:backgroundTint="#b23c17"
                        android:gravity="center"
                        android:padding="17dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/ico_dataprivacy"
                        android:tint="@color/white" />

                    <TextView
                        android:id="@+id/lbldp"
                        android:layout_width="75dp"
                        android:layout_height="20dp"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:text="@string/data_privacy"
                        android:textAlignment="center"
                        android:textColor="@color/colorTextDark"
                        android:textSize="10dp"
                        android:textStyle="normal" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/line33"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="0dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btnfeedback"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_centerHorizontal="true"
                        android:background="@layout/round_corner_image_button_main"
                        android:backgroundTint="#009688"
                        android:gravity="center"
                        android:padding="17dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/ico_feedback"
                        android:tint="@color/white" />

                    <TextView
                        android:id="@+id/lblfeedback"
                        android:layout_width="75dp"
                        android:layout_height="20dp"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:text="@string/feedback"
                        android:textAlignment="center"
                        android:textColor="@color/colorTextDark"
                        android:textSize="10dp"
                        android:textStyle="normal" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/line34"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="0dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

