                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_centerHorizontal="true"
                        android:background="@layout/round_corner_image_button_main"
                        android:backgroundTint="#607d8b"
                        android:gravity="center"
                        android:padding="17dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/ico_refer"
                        android:tint="@color/white" />

                    <TextView
                        android:id="@+id/lblrefer"
                        android:layout_width="75dp"
                        android:layout_height="20dp"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:text="@string/ref_dashboard"
                        android:textAlignment="center"
                        android:textColor="@color/colorTextDark"
                        android:textSize="10dp"
                        android:textStyle="normal" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/line22"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="0dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btnfaq"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_centerHorizontal="true"
                        android:background="@layout/round_corner_image_button_main"
                        android:backgroundTint="#4caf50"
                        android:gravity="center"
                        android:padding="17dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/ico_faq"
                        android:tint="@color/white" />

                    <TextView
                        android:id="@+id/lblfaq"
                        android:layout_width="75dp"
                        android:layout_height="20dp"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:text="@string/f_a_q"
                        android:textAlignment="center"
                        android:textColor="@color/colorTextDark"
                        android:textSize="10dp"
                        android:textStyle="normal" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/line23"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="0dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btnversion"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_centerHorizontal="true"
                        android:background="@layout/round_corner_image_button_main"
                        android:backgroundTint="#cddc39"
                        android:gravity="center"
                        android:padding="17dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/ico_about"
                        android:tint="@color/white" />

                    <TextView
                        android:id="@+id/lblversion"
                        android:layout_width="75dp"
                        android:layout_height="20dp"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:text="@string/about_us"
                        android:textAlignment="center"
                        android:textColor="@color/colorTextDark"
                        android:textSize="10dp"
                        android:textStyle="normal" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/line24"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="0dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btntandc"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_centerHorizontal="true"
                        android:background="@layout/round_corner_image_button_main"
                        android:backgroundTint="#ffeb3b"
                        android:gravity="center"
                        android:padding="17dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/ico_tandc"
                        android:tint="@color/white" />

